%h1 Dashboard
.tabbable
  %ul{:class => "nav nav-pills"}
    %li{:class => "active"}
      %a{:href => '#gendata', :'data-toggle' =>"tab"}
        General Usage Data
    %li
      %a{:href => '#skills', :id => 'skilltab', :'data-toggle' =>"tab"}
        Skills Report
    %li
      %a{:href => '#impact', :id => 'impacttab', :'data-toggle' =>"tab"}
        Impact Report
  .tab-content
    %div{:class => "tab-pane active", :id => "gendata"}
      = render  :partial => 'general_usage_data'
    %div{:class => "tab-pane", :id => "skills"}
      = render  :partial => 'skills_report_data'
    %div{:class => "tab-pane", :id => "impact"}
      = render  :partial => 'impact_report_data'
